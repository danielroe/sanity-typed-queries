{
  "name": "sanity-typed-queries",
  "version": "0.9.0",
  "description": "A collection of helper functions to improve the typing of Sanity resources.",
  "keywords": [
    "sanity",
    "typescript",
    "library",
    "groq"
  ],
  "repository": "danielroe/sanity-typed-queries",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "postinstall": "husky install",
    "lint": "pnpm lint:all:eslint && pnpm lint:all:prettier",
    "lint:all:eslint": "pnpm lint:eslint --ext .js,.ts .",
    "lint:all:prettier": "pnpm lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "husky install && pnpm build --stub",
    "prepublishOnly": "pnpm lint && pnpm test && pnpm build && pinst --disable",
    "postpublish": "pinst --enable",
    "release": "release-it",
    "test": "pnpm test:types && pnpm test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitest/coverage-c8": "^0.24.0",
    "c8": "^7.12.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "esbuild": "0.15.10",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-type": "^0.14.2",
    "groq": "^2.33.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "resolve": "^1.22.1",
    "semver": "^7.3.8",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vite": "^3.1.6",
    "vitest": "~0.24.0"
  },
  "packageManager": "pnpm@7.13.2"
}
