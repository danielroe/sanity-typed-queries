{
  "name": "sanity-typed-queries",
  "version": "0.9.5",
  "description": "A collection of helper functions to improve the typing of Sanity resources.",
  "keywords": [
    "sanity",
    "typescript",
    "library",
    "groq"
  ],
  "repository": "danielroe/sanity-typed-queries",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "pnpm lint:all:eslint && pnpm lint:all:prettier",
    "lint:all:eslint": "pnpm lint:eslint --ext .js,.ts .",
    "lint:all:prettier": "pnpm lint:prettier \"{src,test,docs}/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --log-level warn",
    "dev:prepare": "pnpm build --stub",
    "prepare": "husky install && pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && npm publish",
    "test": "pnpm test:types && pnpm test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/coverage-v8": "2.1.1",
    "bumpp": "9.4.0",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "esbuild": "0.20.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "expect-type": "0.15.0",
    "groq": "3.36.2",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "resolve": "1.22.8",
    "semver": "7.5.4",
    "typescript": "5.3.3",
    "unbuild": "2.0.0",
    "vite": "4.5.5",
    "vitest": "2.1.1"
  },
  "packageManager": "pnpm@9.11.0"
}
