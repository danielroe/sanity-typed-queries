{
  "name": "sanity-typed-queries",
  "version": "0.9.1",
  "description": "A collection of helper functions to improve the typing of Sanity resources.",
  "keywords": [
    "sanity",
    "typescript",
    "library",
    "groq"
  ],
  "repository": "danielroe/sanity-typed-queries",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "postinstall": "husky install",
    "lint": "pnpm lint:all:eslint && pnpm lint:all:prettier",
    "lint:all:eslint": "pnpm lint:eslint --ext .js,.ts .",
    "lint:all:prettier": "pnpm lint:prettier \"{src,test,docs}/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "dev:prepare": "pnpm build --stub",
    "prepare": "husky install && pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test && pinst --disable",
    "postpublish": "pinst --enable",
    "release": "release-it",
    "test": "pnpm test:types && pnpm test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "5.1.1",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@vitest/coverage-c8": "0.26.2",
    "c8": "7.12.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "esbuild": "0.16.12",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect-type": "0.15.0",
    "groq": "2.33.2",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "pinst": "3.0.0",
    "prettier": "2.8.1",
    "release-it": "15.6.0",
    "resolve": "1.22.1",
    "semver": "7.3.8",
    "typescript": "4.9.4",
    "unbuild": "1.0.2",
    "vite": "4.0.3",
    "vitest": "0.26.2"
  },
  "packageManager": "pnpm@7.21.0"
}
