{
  "name": "sanity-typed-queries",
  "version": "0.5.1",
  "description": "A collection of helper functions to improve the typing of Sanity resources.",
  "license": "MIT",
  "repository": "danielroe/sanity-typed-queries",
  "contributors": [
    {
      "name": "Daniel Roe <daniel@roe.dev>",
      "url": "https://github.com/danielroe"
    }
  ],
  "keywords": [
    "sanity",
    "typescript",
    "library",
    "groq"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf lib/*",
    "compile": "tsc",
    "lint": "run-s lint:all:*",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "release": "release-it",
    "test": "run-s test:*",
    "test:types": "tsd",
    "test:unit": "jest"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@release-it/conventional-changelog": "^1.1.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "groq": "^1.149.0",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.3",
    "prettier-eslint-cli": "^5.0.0",
    "release-it": "^13.5.1",
    "tsd": "0.11.0",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "tsd": {
    "directory": "test/tsd",
    "compilerOptions": {
      "rootDir": "."
    }
  }
}
