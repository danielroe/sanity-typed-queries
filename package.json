{
  "name": "sanity-typed-queries",
  "version": "0.6.8",
  "description": "A collection of helper functions to improve the typing of Sanity resources.",
  "keywords": [
    "sanity",
    "typescript",
    "library",
    "groq"
  ],
  "repository": "danielroe/sanity-typed-queries",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Roe <daniel@roe.dev>",
      "url": "https://github.com/danielroe"
    }
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf lib/*",
    "compile": "siroc build",
    "lint": "run-s lint:all:*",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "release": "release-it",
    "test": "run-s test:*",
    "test:types": "tsd",
    "test:unit": "jest"
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "dependencies": {},
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/jest": "^26.0.3",
    "codecov": "^3.7.0",
    "groq": "^1.149.16",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "release-it": "^13.6.3",
    "siroc": "^0.1.2",
    "tsd": "0.11.0",
    "typescript": "^3.9.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "tsd": {
    "directory": "test/tsd",
    "compilerOptions": {
      "rootDir": "."
    }
  }
}
