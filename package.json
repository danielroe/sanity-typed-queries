{
  "name": "sanity-typed-queries",
  "version": "0.8.1",
  "description": "A collection of helper functions to improve the typing of Sanity resources.",
  "keywords": [
    "sanity",
    "typescript",
    "library",
    "groq"
  ],
  "repository": "danielroe/sanity-typed-queries",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "postinstall": "husky install",
    "lint": "npm run lint:all:eslint && npm run lint:all:prettier",
    "lint:all:eslint": "npm run lint:eslint -- --ext .js,.ts .",
    "lint:all:prettier": "npm run lint:prettier -- \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "husky install && npm run build -- --stub",
    "prepublishOnly": "npm run lint && npm run test && pinst --disable",
    "postpublish": "pinst --enable",
    "release": "release-it",
    "test": "npm run test:types && npm run test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitest/coverage-c8": "^0.22.1",
    "c8": "^7.12.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "esbuild": "0.15.6",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "expect-type": "^0.13.0",
    "groq": "^2.29.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "release-it": "^15.4.1",
    "resolve": "^1.22.1",
    "semver": "^7.3.7",
    "typescript": "^4.8.2",
    "unbuild": "^0.8.10",
    "vite": "^3.0.9",
    "vitest": "~0.22.1"
  },
  "packageManager": "pnpm@7.9.5"
}
